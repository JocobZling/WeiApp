Page{
    background-image: url("https://jocobzling.mynatapp.cc/fengling.png");
    background-size: 100% 100%;
}
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.userinfo-avatar {
    width: 128rpx;
    height: 128rpx;
    margin: 20rpx;
    border-radius: 50%;
}

.userinfo-nickname {
    color: #FFFFFF;
}

.usermotto {
    margin-top: 200px;
}

.getininfo {
    position: fixed;
    left: 0;
    bottom: 225rpx;
    width: 100%;
    height: 300rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.getinfo {
    width: 330rpx;
    height: 300rpx;
}

.image {
    position: fixed;
    left: 0;
    bottom: 100rpx;
    width: 100%;
    height: 300rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}
